# Server Configuration
NODE_ENV=production
PORT=3000

# PostgreSQL Database Configuration
# Format: postgresql://username:password@host:port/database
# For Kamal deployment, use the db accessory name as host: db
DATABASE_URL=postgresql://postgres:your_password@db:5432/signal_harvester

# Docker Registry (for Kamal)
KAMAL_REGISTRY_PASSWORD=your_dockerhub_token

# PostgreSQL Accessory Configuration (for Kamal)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_database_password
